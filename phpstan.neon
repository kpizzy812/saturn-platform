parameters:
    level: 0
    paths:
        - app

    # Ignore false positives from Eloquent static calls (without larastan)
    ignoreErrors:
        # Eloquent Model static methods like Server::where(), User::count(), etc.
        -
            message: '#Static call to instance method Illuminate\\Support\\Collection<\(int\|string\),mixed>::(where|whereNotNull|count|pluck|whereIn)\(\)#'
            reportUnmatched: false
