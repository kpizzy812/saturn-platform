APP_ID=
APP_NAME="Saturn Platform"
APP_KEY=

DB_USERNAME=saturn
DB_PASSWORD=

REDIS_PASSWORD=

# Broadcasting / WebSocket Configuration (Soketi)
BROADCAST_DRIVER=pusher
PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
# Backend host - internal Docker network (Laravel -> Soketi)
PUSHER_BACKEND_HOST=saturn-realtime
PUSHER_BACKEND_PORT=6001
PUSHER_SCHEME=https

# Frontend WebSocket Configuration
# IMPORTANT: VITE_PUSHER_HOST must be your PUBLIC domain (not saturn-realtime!)
# Leave VITE_PUSHER_SCHEME empty for auto-detection based on page protocol (http→ws, https→wss)
# For SSL deployments with domain: set VITE_PUSHER_SCHEME=wss and VITE_PUSHER_PORT=443
VITE_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
VITE_PUSHER_HOST=
VITE_PUSHER_PORT=
VITE_PUSHER_SCHEME=

# Error Tracking (Sentry)
# Get DSN from https://sentry.io -> Settings -> Projects -> Client Keys
SENTRY_DSN=
SENTRY_ENABLE_TRACING=true
VITE_SENTRY_DSN="${SENTRY_DSN}"

ROOT_USERNAME=
ROOT_USER_EMAIL=
ROOT_USER_PASSWORD=

REGISTRY_URL=ghcr.io
